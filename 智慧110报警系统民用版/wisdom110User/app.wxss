/**app.wxss**/
@import "/static/style/stylesheet.wxss";
@import "/static/style/animate.min.wxss";
page{
  background-color: #eceff4;
  --themeBackgroundColor:#0082d6;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
.animated{
  animation-duration: .5s;
  -webkit-animation-duration: .5s;
}